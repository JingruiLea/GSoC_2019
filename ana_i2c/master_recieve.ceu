#include "_ana_i2c.ceu"

_Serial.begin(9600);
var byte temp = _;

loop do
    await I2C_Start();
    var[20] byte str = [];
    await I2C_Master_Receive(&str, 8, 2);
    await I2C_Stop();
    var u8 i;
    loop i in [1-> $str as u8] do
        _Serial.println(str[i-1]);
        _Serial.flush();
    end
    await 1s;
end